[{{subdomain}}]
user = {{username}}
group = {{username}}
listen = /var/run/php/php{{php_version}}-fpm-{{subdomain}}.sock
listen.owner = www-data
listen.group = www-data
listen.mode = 0666
pm = dynamic
pm.max_children = 20
pm.start_servers = 3
pm.min_spare_servers = 1
pm.max_spare_servers = 5
pm.process_idle_timeout = 30s
pm.max_requests = 500
request_terminate_timeout = 600